@model List<st10384480AgriConnect.Models.User>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Farmer List</h2>
<table>
    <tr><th>Name</th><th>Email</th></tr>
    @foreach (var f in Model)
    {
        <tr>
            <td>@f.FullName</td>
            <td>@f.Email</td>
        </tr>
    }
</table>